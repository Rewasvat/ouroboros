
%class-name           MDParser
%baseclass-preinclude iostream
%namespace            opwig

%token IDENTIFIER
%token STRING_LITERAL

%%

input:
  /* empty */ { std::cout << "nothing" << std::endl; }
|
  input IDENTIFIER { std::cout << "Token" << std::endl; }
|
  input STRING_LITERAL { std::cout << "String" << std::endl; }
;

